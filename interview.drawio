        <mxCell id="etOJwjgyMubkftZ0emL1-40" value="&lt;ul&gt;&lt;li&gt;Need to manage Node.js installation&lt;/li&gt;&lt;li&gt;All services share same Node.js runtime&lt;/li&gt;&lt;li&gt;Tight coupling&lt;/li&gt;&lt;li&gt;Need to manage web hosting, eg Apache, NGINX, Docker, PM2..&lt;/li&gt;&lt;/ul&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="10" y="1410" width="210" height="130" as="geometry" />
        </mxCell>
        <mxCell id="etOJwjgyMubkftZ0emL1-41" value="&lt;ul&gt;&lt;li&gt;No need to manage Node.js installation&lt;/li&gt;&lt;li&gt;Supports containers&lt;/li&gt;&lt;li&gt;Expensive to host and scale&lt;/li&gt;&lt;/ul&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="274" y="1410" width="210" height="130" as="geometry" />
        </mxCell>
        <mxCell id="etOJwjgyMubkftZ0emL1-42" value="&lt;ul&gt;&lt;li&gt;No need to manage Node.js installation&lt;/li&gt;&lt;li&gt;Supports containers&lt;/li&gt;&lt;li&gt;Recovery options poor&lt;/li&gt;&lt;li&gt;Limited configuration including identity management&lt;/li&gt;&lt;/ul&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="532.5" y="1410" width="210" height="130" as="geometry" />
        </mxCell>
        <mxCell id="etOJwjgyMubkftZ0emL1-43" value="&lt;ul&gt;&lt;li&gt;No need to manage Node.js installation&lt;/li&gt;&lt;li&gt;Offer often behind Node LTS&lt;br&gt;&lt;/li&gt;&lt;li&gt;Node.js does not have as many features as .NET&lt;/li&gt;&lt;li&gt;Premium tier does not have &quot;safe&quot; npm installs&lt;/li&gt;&lt;li&gt;Tight coupling&lt;/li&gt;&lt;li&gt;Supports containers&lt;/li&gt;&lt;/ul&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
